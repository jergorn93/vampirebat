  <item ident="item0">
    <presentation>
      <flow>
        <material>
          <mattext>What does High Density Road refer to?</mattext>
        </material>
        <response_lid ident="MC1" rcardinality="single" rtiming="no">
          <render_choice>
            <flow_label>
              <response_label ident="1">
                <flow_mat>
                  <material>
                    <mattext>More than 6000 vehicles use the road in any average 24 hours period.</mattext>
                  </material>
                </flow_mat>
              </response_label>
            </flow_label>
            <flow_label>
              <response_label ident="2">
                <flow_mat>
                  <material>
                    <mattext>More than 5000 vehicles use the road in any average 24 hours period.</mattext>
